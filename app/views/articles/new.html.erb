<h1 class="m-4">New Article</h1>
<div class="d-flex justify-content-center m-4">
<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
</div>
<div class="d-flex justify-content-center">
<button class="btn btn-outline-black"><%= link_to "Back to articles!", articles_path%></button>
</div>
